<h1>Users</h1>
<table>
  <thead>
    <th>
      <td>ID</td>
      <td>Email</td>
      <td>Actions</td>
    </th>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.id %></td>
        <td><%= user.email %></td>
        <td>
          <%= link_to 'Edit', edit_user_path(user) %>
          <%= link_to 'Destroy', user_path(user), method: :delete %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
